    "babel-core": "^7.0.0-bridge.0",
    "babel-loader": "^7.1.5",
    "babel-plugin-add-module-exports": "^1.0.2",
    "es-check": "^5.1.0",
    "rimraf": "3.0.0",
    "ts-loader": "6.2.1",
    "typescript": "^3.7.3",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10"
  }
