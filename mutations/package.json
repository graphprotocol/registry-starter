  "scripts": {
    "build": "yarn build:package",
    "build:bundle": "rimraf ./bundle && webpack",
    "build:package": "tsc --outDir ./dist && rimraf ./package && mkdir ./package && cp -r ./dist ./package/dist && cp package.json ./package && cp -r ./src ./package/src",
